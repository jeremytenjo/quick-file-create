{
	"name": "quick-file-create",
	"displayName": "Quick File Create",
	"description": "Automatically create a file inside created folder",
	"publisher": "JeremyTenjo",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:sftp"
	],
	"main": "./src/index.js",
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "",
		"publish": "vsce publish minor",
		"vscode:prepublish": "npm run test"
	},
	"icon": "assets/images/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeremytenjo/quick-file-create"
	},
	"contributes": {
		"configuration": {
			"title": "QuickFileCreate",
			"properties": {
				"quickFileCreate.fileNameCase": {
					"type": "string",
					"default": "camel",
					"description": "Created file name case"
				},
			}
		}
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"os": "^0.1.1"
	}
}